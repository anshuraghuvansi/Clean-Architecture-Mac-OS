# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Clean Architecture Mac OS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'RxSwift', '~> 5'
  pod 'RxCocoa', '~> 5'

end

target 'DomainKit' do
  use_frameworks!
  
  pod 'RxSwift', '~> 5'
  
end

target 'NetworkKit' do
  use_frameworks!
  
  pod 'RxSwift', '~> 5'
  pod 'Alamofire'
  
end
